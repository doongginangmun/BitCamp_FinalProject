--------------------------------------------------------
--  DDL for Table CART
--------------------------------------------------------

  CREATE TABLE "CART" 
   (	"MEMBER_ID" VARCHAR2(100 BYTE), 
	"QUANTITY" NUMBER, 
	"CART_ID" NUMBER(10,0) DEFAULT 1, 
	"OPTION_NAME" VARCHAR2(100 BYTE), 
	"PRODUCT_NAME" VARCHAR2(100 BYTE)
   ) ;

   COMMENT ON COLUMN "CART"."MEMBER_ID" IS '회원 아이디';
   COMMENT ON COLUMN "CART"."QUANTITY" IS '수량';
   COMMENT ON COLUMN "CART"."OPTION_NAME" IS '옵션 이름';
   COMMENT ON TABLE "CART"  IS '장바구니';
--------------------------------------------------------
--  DDL for Table FAQ
--------------------------------------------------------

  CREATE TABLE "FAQ" 
   (	"FAQ_NUM" NUMBER, 
	"QNA_CATEGORY_NUM" NUMBER, 
	"TITLE" VARCHAR2(200 BYTE), 
	"CONTENTS" VARCHAR2(4000 BYTE)
   ) ;

   COMMENT ON COLUMN "FAQ"."FAQ_NUM" IS 'FAQ 번호';
   COMMENT ON COLUMN "FAQ"."QNA_CATEGORY_NUM" IS '질문 카테고리 번호';
   COMMENT ON COLUMN "FAQ"."TITLE" IS '제목';
   COMMENT ON COLUMN "FAQ"."CONTENTS" IS '내용';
   COMMENT ON TABLE "FAQ"  IS '자주하는 질문';
--------------------------------------------------------
--  DDL for Table ITEM
--------------------------------------------------------

  CREATE TABLE "ITEM" 
   (	"ITEM_NUM" NUMBER, 
	"PRICE" NUMBER, 
	"STOCK" NUMBER, 
	"SALES" NUMBER DEFAULT 0, 
	"DISCOUNT" NUMBER, 
	"REG_DATE" DATE DEFAULT sysdate, 
	"UNIT_PRICE" NUMBER, 
	"PRODUCT_ID" VARCHAR2(20 BYTE)
   ) ;

   COMMENT ON COLUMN "ITEM"."ITEM_NUM" IS '제품 번호';
   COMMENT ON COLUMN "ITEM"."PRICE" IS '가격';
   COMMENT ON COLUMN "ITEM"."STOCK" IS '재고';
   COMMENT ON COLUMN "ITEM"."SALES" IS '판매';
   COMMENT ON COLUMN "ITEM"."DISCOUNT" IS '할인률';
   COMMENT ON COLUMN "ITEM"."REG_DATE" IS '제품 등록일자';
   COMMENT ON COLUMN "ITEM"."UNIT_PRICE" IS '단위 가격';
   COMMENT ON COLUMN "ITEM"."PRODUCT_ID" IS '상품 아이디';
   COMMENT ON TABLE "ITEM"  IS '제품';
--------------------------------------------------------
--  DDL for Table MEMBER
--------------------------------------------------------

  CREATE TABLE "MEMBER" 
   (	"MEMBER_ID" VARCHAR2(100 BYTE), 
	"PASSWORD" VARCHAR2(50 BYTE), 
	"MEMBER_NAME" VARCHAR2(100 BYTE), 
	"EMAIL" VARCHAR2(200 BYTE), 
	"PHONE" VARCHAR2(20 BYTE), 
	"ZIPNO" VARCHAR2(500 BYTE), 
	"LOAD_ADDRESS" VARCHAR2(500 BYTE), 
	"JIBUN_ADDRESS" VARCHAR2(500 BYTE), 
	"REST_ADDRESS" VARCHAR2(500 BYTE), 
	"MEMBER_DATE" DATE DEFAULT SYSDATE, 
	"BIRTHDAY" VARCHAR2(20 BYTE), 
	"PRE_SCORE" NUMBER, 
	"THIS_SCORE" NUMBER, 
	"POINT" NUMBER DEFAULT 0, 
	"MEMBER_STATUS" VARCHAR2(20 BYTE) DEFAULT '일반'
   ) ;
--------------------------------------------------------
--  DDL for Table MEMBER_DETAIL
--------------------------------------------------------

  CREATE TABLE "MEMBER_DETAIL" 
   (	"MEMBER_ID" VARCHAR2(20 BYTE), 
	"LANGUAGE" VARCHAR2(20 BYTE), 
	"BANNER_OPTION" NUMBER
   ) ;

