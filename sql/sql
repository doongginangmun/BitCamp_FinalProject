--  DDL for Table ORDERS
--------------------------------------------------------

  CREATE TABLE "ORDERS" 
   (	"ORDER_NUM" VARCHAR2(100 BYTE), 
	"TOTALPRICE" NUMBER, 
	"APPLY_NUM" VARCHAR2(100 BYTE), 
	"ORDER_STATUS" VARCHAR2(20 BYTE), 
	"PG" VARCHAR2(20 BYTE), 
	"ORDER_DATE" DATE DEFAULT sysdate, 
	"PAID_AT" NUMBER, 
	"MEMBER_ID" VARCHAR2(100 BYTE), 
	"MEMBER_NAME" VARCHAR2(100 BYTE), 
	"EMAIL" VARCHAR2(200 BYTE), 
	"PHONE" VARCHAR2(20 BYTE), 
	"ZIPNO" VARCHAR2(500 BYTE), 
	"TIMESTAMP" DATE, 
	"DELIVERY_REQUEST" VARCHAR2(500 BYTE), 
	"ADDRESS" VARCHAR2(1000 BYTE)
   ) ;

   COMMENT ON COLUMN "ORDERS"."ORDER_NUM" IS '주문 번호';
   COMMENT ON COLUMN "ORDERS"."TOTALPRICE" IS '총 가격';
   COMMENT ON COLUMN "ORDERS"."APPLY_NUM" IS '카드사 승인번호';
   COMMENT ON COLUMN "ORDERS"."ORDER_STATUS" IS '주문 상태';
   COMMENT ON COLUMN "ORDERS"."PG" IS 'PG';
   COMMENT ON COLUMN "ORDERS"."ORDER_DATE" IS '주문 날짜';
   COMMENT ON COLUMN "ORDERS"."PAID_AT" IS '결제 승인시각';
   COMMENT ON COLUMN "ORDERS"."MEMBER_ID" IS '회원 아이디';
   COMMENT ON COLUMN "ORDERS"."MEMBER_NAME" IS '회원 이름';
   COMMENT ON COLUMN "ORDERS"."EMAIL" IS '이메일';
   COMMENT ON COLUMN "ORDERS"."PHONE" IS '전화번호';
   COMMENT ON COLUMN "ORDERS"."ZIPNO" IS '우편번호';
   COMMENT ON COLUMN "ORDERS"."TIMESTAMP" IS '처리일자';
   COMMENT ON COLUMN "ORDERS"."DELIVERY_REQUEST" IS '배송 요청';
   COMMENT ON TABLE "ORDERS"  IS '주문정보';
